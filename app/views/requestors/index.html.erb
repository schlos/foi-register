<h1>Listing requestors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @requestors.each do |requestor| %>
  <tr>
    <td><%= requestor.name %></td>
    <td><%= requestor.email %></td>
    <td><%= link_to 'Show', requestor %></td>
    <td><%= link_to 'Edit', edit_requestor_path(requestor) %></td>
    <td><%= link_to 'Destroy', requestor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Requestor', new_requestor_path %>
